PROGRAM BACKDERET2;
USES CRT;
VAR
    A,B: INTEGER;

PROCEDURE BUATDERET(X,Y: INTEGER);
VAR
    B: INTEGER;
BEGIN
    FOR B := Y TO X DO
    BEGIN
        IF (X >= Y) THEN BEGIN
            WRITE(X, ' ');
            X := X - 2 ;
        END;
    END;
END;

BEGIN
    WRITELN('PROGRAM DERET SEDERHANA');
    WRITE('MASUKKAN NILAI AWAL DERET: ');
    READLN(A);
    WRITE('MASUKKAN NILAI AKHIR DERET: ');
    READLN(B);
    BUATDERET(A, B);
END.